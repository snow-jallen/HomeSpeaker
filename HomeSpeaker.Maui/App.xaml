<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:HomeSpeaker.Maui"
             x:Class="HomeSpeaker.Maui.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <x:String x:Key="AddIcon">&#x2b;</x:String>
            <x:String x:Key="DownloadIcon">&#xf019;</x:String>
            <x:String x:Key="VolumeIcon">&#xf027;</x:String>
            <x:String x:Key="LeftArrow">&#xf060;</x:String>
            <x:String x:Key="CaretDown">&#xf0d7;</x:String>
            <x:String x:Key="CaretUp">&#xf0d8;</x:String> 


        </ResourceDictionary>
    </Application.Resources>
</Application>
